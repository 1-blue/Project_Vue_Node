===== 학생테이블 =====
CREATE TABLE `students` (
  `id` int unsigned NOT NULL AUTO_INCREMENT,
  `name` varchar(10) NOT NULL,
  `yy` varchar(30) DEFAULT NULL,
  `phoneNumber` varchar(13) DEFAULT NULL,
  `email` varchar(25) DEFAULT NULL,
  `birth` varchar(10) NOT NULL,
  `address` varchar(5) NOT NULL,
  `createdAt` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  `updatedAt` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=1001 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

===== clubs =====
CREATE TABLE clubs(
	id int unsigned not null auto_increment primary key,
	name varchar(31) not null,
	createdAt timestamp not null default current_timestamp,
	leader int unsigned,
	constraint foreign key fk_leader_students(leader) references students(id)
);

===== professors =====
professors | CREATE TABLE `professors` (
  `id` int NOT NULL AUTO_INCREMENT,
  `name` varchar(31) NOT NULL,
  `likeCnt` smallint DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

===== subjects =====
create table subjects(
	id integer not null auto_increment primary key,
	name varchar (31) not null,
	professorId integer not null default 1,
	constraint foreign key fk_subject_professor(professorId) references professors(id) on delete set default
);

===== enroll =====
create table enroll(
	id integer not null auto_increment primary key,
	subjectId integer not null,
	studentId integer unsigned not null,
	constraint foreign key fk_enroll_subject(subjectId) references subjects(id) on delete set default,
	constraint foreign key fk_enroll_student(studentId) references students(id) on delete set default
);